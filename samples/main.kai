#library "c" libc

printf :: (s: string) -> void #foreign libc "printf"
putchar :: (c: u8) -> void #foreign libc "putchar"

main :: () -> void {

    lower : u8 = 65
    upper : u8 = 97
    for lower <= 90 && upper <= 122 {
        putchar(lower)
        putchar(upper)
        printf("\n")
        lower = lower + 1
        upper = upper + 1
    }

    for c : u8 = 65; c < 91; c = c + 1 {
        putchar(c)
    }
}

