
tau :: 6.2831853071

pi  :: 3.14

circumference :: (r: f64) -> f64 {
    return tau * r
}

sum :: (x, y, z: f32) -> (f32, err) {
    return x + y + z, nil
}

main :: () -> void {
    c :: circumference(1)
    printf("The circumference of a unit circle is %\n", c)

    j, err := json.parse("{'key': 'value'}")
    if err != nil panic(err)

    print(j)
}

