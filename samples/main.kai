#library "c" libc

printf :: (s: string) -> void #foreign libc "printf"
putchar :: (c: u8) -> void #foreign libc "putchar"

addFive :: (n: u8) -> u8 {
    return n + 5
}

main :: () -> void {

    printf("\n --- START ---\n")

    x : u8 = 65
    x = addFive(x)
    putchar(x)
    putchar(u8(66))

    printf("\n ---  END  ---\n")
}

