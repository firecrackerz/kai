
#library "c" libc

print :: (string, ..any) -> void #foreign libc "printf"

main :: () -> void {

    bar :: () -> u8, u8 {
        return 2, 4
    }

    baz :: () -> u8, u8 {
        return 6, 8
    }

    b1, b2 := bar()
    print("b1: %d, b2: %d\n", b1, b2)

    b1, b2 = baz()
    print("b1: %d, b2: %d\n", b1, b2)
}

