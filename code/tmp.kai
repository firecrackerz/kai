
#import kai("posix")

Foo :: struct {
    a, b, c: i64
}

main :: fn() -> i32 {

    ty := typeof(cast(i64) 5)
    posix.printf("%d\n".raw, ty.Tag)
    posix.printf("%d\n".raw, ty.Integer.Signed)
    posix.printf("%d\n".raw, ty.Integer.Width)

    ty = typeof(cast(f32) 5.0)
    posix.printf("%d\n".raw, ty.Tag)
    posix.printf("%d\n".raw, ty.Float.Width)

    return 5
}

